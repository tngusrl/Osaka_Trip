<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.osaka_trip">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher1_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MemoUpdateActivity"></activity>
        <activity android:name=".TipActivity" />
        <activity android:name=".FavoritesActivity" />
        <activity android:name=".TripinfolistActivity" />
        <activity android:name=".TripinfoActivity" />
        <activity android:name=".ConvActivity" />
        <activity android:name=".ConvListActivity" />
        <activity android:name=".MemoActivity" />
        <activity
            android:name=".MemolistActivity"
            android:label="@string/title_activity_memolist"
            android:theme="@style/MemolistActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/ads_APPLICATION_ID" />
    </application>

</manifest>